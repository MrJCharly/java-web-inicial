<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-schema="public" match-name="clientes"/>
  <table-filter match-schema="public" match-name="cuentas_bancarias"/>
  <table-filter match-schema="public" match-name="movimientos"/>
  
  <table name="clientes" schema="public" class="ar.edu.unju.virtual.model.domain.Cliente"></table>
  <table name="cuentas_bancarias" schema="public" class="ar.edu.unju.virtual.model.domain.Cuenta"></table>
  <table name="movimientos" schema="public" class="ar.edu.unju.virtual.model.domain.Movimiento"></table>
</hibernate-reverse-engineering>